{
    "name": "@lynkify/backend",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "exports": {
        "./src/trpc/router": {
            "types": "./dist/trpc/router.d.ts",
            "import": "./dist/trpc/router.js"
        }
    },
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc --build",
        "start": "node dist/index.js",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist tsconfig.tsbuildinfo",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:studio": "prisma studio"
    },
    "dependencies": {
        "@lynkify/shared": "workspace:*",
        "@prisma/client": "^6.3.1",
        "@trpc/server": "^11.0.0-rc.682",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "ioredis": "^5.4.2",
        "superjson": "^2.2.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "@types/cors": "^2.8.17",
        "@types/node": "^22.13.4",
        "eslint": "^9.14.0",
        "prisma": "^6.3.1",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.24.1"
    }
}